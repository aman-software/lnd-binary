{
  "name": "brocoin-ln",
  "version": "0.3.24",
  "description": "Download the latest lnd binary",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=4.0.0"
  },
  "scripts": {
    "test": "babel-tape-runner test/*.js | tap-spec",
    "lint": "prettier-eslint --write \"./**/*.{js,json,md}\"",
    "build-src": "babel src --out-dir dist",
    "build-lnd-bin": "mkdir -p dist/bin && babel src/bin/lnd --out-file dist/bin/lnd  && chmod +xx dist/bin/lnd",
    "build-lnd-install-bin": "mkdir -p dist/bin && babel src/bin/lnd-install --out-file dist/bin/lnd-install && chmod +xx dist/bin/lnd-install",
    "build": "npm run build-src && npm run build-lnd-bin && npm run build-lnd-install-bin",
    "clean": "rm -r ./dist",
    "preversion": "npm test",
    "prepack": "npm run build",
    "postinstall": "postinstall-build dist && node ./dist/bin/lnd-install"
  },
  "files": [
    ".babelrc",
    "config",
    "dist",
    "lib"
  ],
  "bin": {
    "lnd": "./dist/bin/lnd",
    "lnd-install": "./dist/bin/lnd-install"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aman-software/lnd-binary.git"
  },
  "dependencies": {
    "axios": "0.24.0",
    "cacache": "15.3.0",
    "debug": "4.3.2",
    "find-config": "1.0.0",
    "fs-extra": "10.0.0",
    "go-platform": "1.0.0",
    "gunzip-maybe": "1.4.2",
    "hasha": "5.2.2",
    "npmlog": "6.0.0",
    "postinstall-build": "5.0.3",
    "source-map-support": "0.5.21",
    "tar-fs": "2.1.1",
    "true-case-path": "2.2.1",
    "unzip-stream": "0.3.1"
  },
  "keywords": [
    "bitcoin",
    "lightning-network",
    "lnd"
  ],
  "author": "aman saxena",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aman-software/lnd-binary.git"
  },
  "homepage": "https://github.com/aman-software/lnd-binary.git",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-plugin-source-map-support": "2.1.3",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "babel-tape-runner": "3.0.0",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "prettier": "2.2.1",
    "prettier-eslint": "12.0.0",
    "prettier-eslint-cli": "5.0.0",
    "tap-spec": "5.0.0",
    "tape": "5.3.2",
    "tape-promise": "4.0.0"
  },
  "buildDependencies": [
    "babel-cli",
    "babel-plugin-add-module-exports",
    "babel-preset-env"
  ]
}
